{
    "Name": "char.name",
    "CLASS  LEVEL": "char.class",
    "RACE": "char.race",
    "ALIGNMENT": "char.alignment",
    "BACKGROUND": "char.background",
    "AGE": "char.description.age ?? ''",
    "HEIGHT": "char.description.height ?? ''",
    "WEIGHT": "char.description.weight ?? ''",
    "EYES": "char.description.eyes ?? ''",
    "SKIN": "char.description.skin ?? ''",
    "HAIR": "char.description.hair ?? ''",
    "Prof": "modString(char.proficiencyBonus)",
    "PasPer": "10 + char.skills.Perception",
    "StrMod2": "modFromScore(char.abilityScores.Strength)",
    "Str": "char.abilityScores.Strength",
    "DexMod": "modFromScore(char.abilityScores.Dexterity)",
    "Dex": "char.abilityScores.Dexterity",
    "ConMod": "modFromScore(char.abilityScores.Constitution)",
    "Con": "char.abilityScores.Constitution",
    "IntMod": "modFromScore(char.abilityScores.Intelligence)",
    "Int": "char.abilityScores.Intelligence",
    "WisMod": "modFromScore(char.abilityScores.Wisdom)",
    "Wis": "char.abilityScores.Wisdom",
    "ChaMod": "modFromScore(char.abilityScores.Charisma)",
    "Cha": "char.abilityScores.Charisma",
    "Skill1": "modString(char.saves.Strength)",
    "Skill2": "char.skills.Athletics",
    "Skill3": "modString(char.skills.Intimidation-char.abilityScores.Charisma+char.abilityScores.Strength)",
    "Skill4": "modString(char.saves.Dexterity)",
    "Skill5": "char.skills.Acrobatics",
    "Skill6": "modString(char.skills['Sleight of Hand'])",
    "Skill7": "modString(char.skills.Stealth)",
    "Skill8": "modString(char.saves.Constitution)",
    "Skill9": "modString(char.saves.Intelligence)",
    "Skill10": "char.skills.Arcana",
    "Skill11": "char.skills.History",
    "Skill12": "char.skills.Investigation",
    "Skill13": "char.skills.Nature",
    "Skill14": "char.skills.Religion",
    "Skill15": "modString(char.saves.Wisdom)",
    "Skill16": "char.skills['Animal Handling']",
    "Skill17": "char.skills.Insight",
    "Skill18": "char.skills.Medicine",
    "Skill19": "char.skills.Perception",
    "Skill20": "char.skills.Survival",
    "Skill21": "modString(char.saves.Charisma)",
    "Skill22": "char.skills.Deception",
    "Skill23": "char.skills.Intimidation",
    "Skill24": "char.skills.Performance",
    "Skill25": "char.skills.Persuasion",
    "AC": "char.armorClass",
    "Init": "modString(char.initiative)"
}
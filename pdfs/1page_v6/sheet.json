{
    "name": "char.name",
    "class_lvl": "char.class",
    "race": "char.race",
    "align": "char.alignment",
    "Strength": "modFromScore(char.abilityScores.Strength)",
    "Strength Mod": "char.abilityScores.Strength",
    "Str sav": "modString(char.saves.Strength)",
    "Dex": "modFromScore(char.abilityScores.Dexterity)",
    "Dex Mod": "char.abilityScores.Dexterity",
    "Dex save": "modString(char.saves.Dexterity)",
    "Const": "modFromScore(char.abilityScores.Constitution)",
    "Const Mod": "char.abilityScores.Constitution",
    "Const sav": "modString(char.saves.Constitution)",
    "Int": "modFromScore(char.abilityScores.Intelligence)",
    "Int Mod": "char.abilityScores.Intelligence",
    "Int sav": "modString(char.saves.Intelligence)",
    "WIS": "modFromScore(char.abilityScores.Wisdom)",
    "Wis Mod": "char.abilityScores.Wisdom",
    "Wis save": "modString(char.saves.Wisdom)",
    "Cha": "modFromScore(char.abilityScores.Charisma)",
    "Cha mod": "char.abilityScores.Charisma",
    "cha save": "modString(char.saves.Charisma)",
    "Proficiency Bonus": "modString(char.proficiencyBonus)",
    "athletics": "modString(char.skills.Athletics)",
    "Acro": "modString(char.skills.Acrobatics)",
    "sleight": "modString(char.skills['Sleight of Hand'])",
    "stealth": "modString(char.skills.Stealth)",
    "Arcana": "modString(char.skills.Arcana)",
    "History": "modString(char.skills.History)",
    "Invest": "modString(char.skills.Investigation)",
    "Nature": "modString(char.skills.Nature)",
    "Religion": "modString(char.skills.Religion)",
    "animal hand": "modString(char.skills['Animal Handling'])",
    "insight": "modString(char.skills.Insight)",
    "Med": "modString(char.skills.Medicine)",
    "perce": "modString(char.skills.Perception)",
    "survival": "modString(char.skills.Survival)",
    "deception": "modString(char.skills.Deception)",
    "intimidation": "modString(char.skills.Intimidation)",
    "perfor": "modString(char.skills.Performance)",
    "persuasion": "modString(char.skills.Persuasion)",
    "Perception": "10 + char.skills.Perception",
    "t1": "char.proficiencies.saves.includes('strength')",
    "t2": "char.proficiencies.skills.includes('athletics')",
    "t3": "char.proficiencies.saves.includes('dexterity')",
    "t4": "char.proficiencies.skills.includes('acrobatics')",
    "t5": "char.proficiencies.skills.includes('sleight of hand')",
    "t6": "char.proficiencies.skills.includes('stealth')",
    "t7": "char.proficiencies.saves.includes('constitution')",
    "t8": "char.proficiencies.saves.includes('intelligence')",
    "t9": "char.proficiencies.skills.includes('arcana')",
    "t10": "char.proficiencies.skills.includes('history')",
    "t11": "char.proficiencies.skills.includes('investigation')",
    "t12": "char.proficiencies.skills.includes('nature')",
    "t13": "char.proficiencies.skills.includes('religion')",
    "t14": "char.proficiencies.saves.includes('wisdom')",
    "t15": "char.proficiencies.skills.includes('animal handling')",
    "t16": "char.proficiencies.skills.includes('insight')",
    "t17": "char.proficiencies.skills.includes('medicine')",
    "t18": "char.proficiencies.skills.includes('perception')",
    "t19": "char.proficiencies.skills.includes('survival')",
    "t20": "char.proficiencies.saves.includes('charisma')",
    "t21": "char.proficiencies.skills.includes('deception')",
    "t22": "char.proficiencies.skills.includes('intimidation')",
    "t23": "char.proficiencies.skills.includes('performance')",
    "t24": "char.proficiencies.skills.includes('persuasion')",
    "languages": "char.proficiencies.other.join('\\\r\\\n ')",
    "machp": "char.baseHitPoints"
}